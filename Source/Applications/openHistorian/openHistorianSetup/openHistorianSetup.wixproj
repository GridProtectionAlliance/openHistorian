    </ReadLinesFromFile>
    <Exec Command="Powershell -ExecutionPolicy ByPass -File &quot;$(ProjectDir)\VersionBitmap.ps1&quot; &quot;$(ProjectDir)\openHistorianSetupDialog.bmp&quot; &quot;@(VersionNumber)&quot;" />
  </Target>
