{
  "type": "datasource",
  "name": "TestData DB",
  "id": "testdata",

  "metrics": true,
  "logs": true,
  "alerting": true,
  "annotations": true,

  "queryOptions": {
    "minInterval": true,
    "maxDataPoints": true
  },

  "info": {
    "description": "Generates test data in different forms",
    "author": {
      "name": "Grafana Project",
      "url": "https://grafana.com"
    },
    "logos": {
      "small": "img/testdata.svg",
      "large": "img/testdata.svg"
    }
  },

  "includes": [
    {
      "type": "dashboard",
      "name": "Streaming Example",
      "path": "dashboards/streaming.json"
    }
  ]
}
